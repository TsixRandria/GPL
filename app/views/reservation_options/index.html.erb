<p id="notice"><%= notice %></p>

<h1>Reservation Options</h1>

<table>
  <thead>
    <tr>
      <th>Quantite</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reservation_options.each do |reservation_option| %>
      <tr>
        <td><%= reservation_option.quantite %></td>
        <td><%= link_to 'Show', reservation_option %></td>
        <td><%= link_to 'Edit', edit_reservation_option_path(reservation_option) %></td>
        <td><%= link_to 'Destroy', reservation_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reservation Option', new_reservation_option_path %>
